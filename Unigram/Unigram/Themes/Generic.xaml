                        <FontIcon x:Name="Star2" FontSize="{TemplateBinding FontSize}" Margin="{TemplateBinding Padding}" Foreground="{ThemeResource TextControlBorderBrush}" Glyph="&#xE1CE;" IsHitTestVisible="False"/>
                        <FontIcon x:Name="Star3" FontSize="{TemplateBinding FontSize}" Margin="{TemplateBinding Padding}" Foreground="{ThemeResource TextControlBorderBrush}" Glyph="&#xE1CE;" IsHitTestVisible="False"/>
                        <FontIcon x:Name="Star4" FontSize="{TemplateBinding FontSize}" Margin="{TemplateBinding Padding}" Foreground="{ThemeResource TextControlBorderBrush}" Glyph="&#xE1CE;" IsHitTestVisible="False"/>
