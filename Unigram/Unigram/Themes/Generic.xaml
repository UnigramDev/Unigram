    <Style TargetType="local:PageHeader">
        <Setter Property="Background" Value="{ThemeResource TelegramBackgroundTitlebarBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:PageHeader">
                    <StackPanel>
