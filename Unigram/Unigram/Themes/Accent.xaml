<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="using:System">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="MessageForegroundBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="MessageBackgroundBrush" Color="#1F2C36"/>
            <SolidColorBrush x:Key="MessageBorderBrush" Color="#144D7B29"/>
            <SolidColorBrush x:Key="MessageSubtleLabelBrush" Color="#a0acb6"/>
            <SolidColorBrush x:Key="MessageSubtleGlyphBrush" Color="#a0acb6"/>

            <StaticResource x:Key="MessageSubtleForegroundBrush" ResourceKey="SystemControlDisabledChromeDisabledLowBrush"/>

            <SolidColorBrush x:Key="MessageHeaderForegroundBrush" Color="#0e7acd"/>
            <SolidColorBrush x:Key="MessageHeaderForegroundBrush2" Color="#0e7acd"/>
            <SolidColorBrush x:Key="MessageHeaderBorderBrush" Color="#2fa9e2"/>

            <SolidColorBrush x:Key="MessageMediaForegroundBrush" Color="#ffffff"/>
            <SolidColorBrush x:Key="MessageMediaBackgroundBrush" Color="#007ee5"/>

            <SolidColorBrush x:Key="MessageHyperlinkForegroundBrush" Color="#FFFFFF"/>

            <SolidColorBrush x:Key="MessageOverlayBackgroundBrush" Color="#54000000"/>

            <SolidColorBrush x:Key="MessageServiceBackgroundBrush" Color="#7F89A0B4"/>
            <SolidColorBrush x:Key="MessageServiceBackgroundPressedBrush" Color="#A2BBC8D4"/>

            <SolidColorBrush x:Key="MessageCallForegroundBrush" Color="Green"/>
            <SolidColorBrush x:Key="MessageCallMissedForegroundBrush" Color="Red"/>

            <HorizontalAlignment x:Key="MessageHorizontalAlignment">Left</HorizontalAlignment>

            <ElementTheme x:Key="MessageTextBlockTheme">Dark</ElementTheme>









            <StaticResource x:Key="TelegramBackgroundTitlebarBrush" ResourceKey="CommandBarBackground"/>
            <StaticResource x:Key="TelegramBackgroundSubtitleBarBrush" ResourceKey="CommandBarOverflowPresenterBackground"/>
            <StaticResource x:Key="ComboBoxPlaceHolderForeground" ResourceKey="TextControlPlaceholderForeground"/>

            <SolidColorBrush x:Key="TelegramDialogGlyphStateBrush" Color="{ThemeResource SystemAccentColorLight2}"/>
            <SolidColorBrush x:Key="TelegramDialogUnreadBadgeMutedBrush" Color="#444444"/>

            <BitmapImage x:Key="DefaultBackgroundBrush" UriSource="ms-appx:///Assets/Images/DefaultBackground.theme-dark.png"/>

        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="MessageForegroundBrush" Color="#000000"/>
            <SolidColorBrush x:Key="MessageBackgroundBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="MessageBorderBrush" Color="#29748ea2"/>
            <SolidColorBrush x:Key="MessageSubtleLabelBrush" Color="#a0acb6"/>
            <SolidColorBrush x:Key="MessageSubtleGlyphBrush" Color="#a0acb6"/>

            <StaticResource x:Key="MessageSubtleForegroundBrush" ResourceKey="SystemControlDisabledChromeDisabledLowBrush"/>

            <SolidColorBrush x:Key="MessageHeaderForegroundBrush" Color="#0e7acd"/>
            <SolidColorBrush x:Key="MessageHeaderForegroundBrush2" Color="#0e7acd"/>
            <SolidColorBrush x:Key="MessageHeaderBorderBrush" Color="#2fa9e2"/>

            <SolidColorBrush x:Key="MessageMediaForegroundBrush" Color="#ffffff"/>
            <SolidColorBrush x:Key="MessageMediaBackgroundBrush" Color="#007ee5"/>

            <SolidColorBrush x:Key="MessageHyperlinkForegroundBrush" Color="#000000"/>

            <SolidColorBrush x:Key="MessageOverlayBackgroundBrush" Color="#54000000"/>

            <SolidColorBrush x:Key="MessageServiceBackgroundBrush" Color="#7F89A0B4"/>
            <SolidColorBrush x:Key="MessageServiceBackgroundPressedBrush" Color="#A2BBC8D4"/>

            <SolidColorBrush x:Key="MessageCallForegroundBrush" Color="Green"/>
            <SolidColorBrush x:Key="MessageCallMissedForegroundBrush" Color="Red"/>

            <HorizontalAlignment x:Key="MessageHorizontalAlignment">Left</HorizontalAlignment>

            <ElementTheme x:Key="MessageTextBlockTheme">Light</ElementTheme>









            <StaticResource x:Key="TelegramBackgroundTitlebarBrush" ResourceKey="CommandBarBackground"/>
            <StaticResource x:Key="TelegramBackgroundSubtitleBarBrush" ResourceKey="CommandBarOverflowPresenterBackground"/>
            <StaticResource x:Key="ComboBoxPlaceHolderForeground" ResourceKey="TextControlPlaceholderForeground"/>

            <SolidColorBrush x:Key="TelegramDialogGlyphStateBrush" Color="{ThemeResource SystemAccentColorLight1}"/>
            <SolidColorBrush x:Key="TelegramDialogUnreadBadgeMutedBrush" Color="#C7C7C7"/>

            <BitmapImage x:Key="DefaultBackgroundBrush" UriSource="ms-appx:///Assets/Images/DefaultBackground.theme-light.png"/>

        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>


    <Thickness x:Key="MessageContentPadding">8,4,10,6</Thickness>
    <Thickness x:Key="MessageContentMargin">0,2,0,0</Thickness>
    <CornerRadius x:Key="MessageCornerRadius">0</CornerRadius>

    <SolidColorBrush x:Key="TelegramDialogLabelStateBrush" Color="#64BC54"/>
    <SolidColorBrush x:Key="TelegramBackgroundAccentBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramForegroundAccentBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramBackgroundMessageBubbleBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramDialogLabelFromBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramDialogLabelDraftBrush" Color="#DD4B39"/>
    <SolidColorBrush x:Key="TelegramDialogUnreadBadgeBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramDialogUnreadLabelBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TelegramDialogVerifiedForegroundBrush" Color="{ThemeResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="TelegramDialogVerifiedBackgroundBrush" Color="#FFFFFF"/>
</ResourceDictionary>
