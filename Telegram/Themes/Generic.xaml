                                        <Setter Target="LayoutRoot.Background"
                                                Value="{ThemeResource MessageReactionChosenBackgroundBrush}" />
                                        <Setter Target="RecentChoosers.BorderBrush"
                                                Value="{ThemeResource MessageReactionChosenBackgroundBrush}" />
